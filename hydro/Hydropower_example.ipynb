        temp=mdl_E_exp[ind_th][0].predict(x_in)
        theta=thetas[ind_th]
        if theta != 0:
            temp = (-1./theta)*np.log(temp)
        plt.plot(M_t_samp, temp, label=r"$\theta = " + str(theta) + "$")

    plt.title("Value function at t=0")
    plt.xlabel("Volume in the reservoir")
    plt.legend(loc="upper left")
    plt.tight_layout()
